{
  "name": "reactapp",
  "version": "1.0.0",
  "description": "Dashboard Application",
  "main": "server.js",
  "engines": {
    "node": ">=4.2.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "test": "mocha \"specs/**/*.spec.js\" --require specs/setup.js --require @babel/register --ignore-styles \"specs/**/*.spec@(.js|.jsx)\"",
    "build": "webpack -p --progress",
    "clean": "babel-node tools/run clean",
    "devRun": "better-npm-run devRun",
    "prodBuild": "better-npm-run prodBuild && webpack -p --progress",
    "liveRun": "better-npm-run liveRun",
    "eslint": "eslint --ext .js --fix --ext .jsx core specs --fix",
    "eslint:fix": "eslint -- --fix --ext .js core",
    "lint": "npm run eslint"
  },
  "betterScripts": {
    "devRun": {
      "command": "babel-node tools/run build && node \"build/server.js\"",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "prodBuild": {
      "command": "babel-node tools/run build",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "liveRun": {
      "command": "webpack-dev-server --hot --history-api-fallback --inline --progress --colors --host 0.0.0.0 --port 9001",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/web-projects/REACT_REDUX.git"
  },
  "keywords": [
    "ReactRedux",
    "Dashboards",
    "Reporting"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/node": "^7.15.4",
    "@babel/plugin-transform-modules-commonjs": "^7.17.9",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-stage-0": "^7.8.3",
    "@babel/register": "^7.15.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "better-npm-run": "^0.1.1",
    "chai": "^4.2.0",
    "chai-enzyme": "^1.0.0-beta.1",
    "cheerio": "^1.0.0-rc.10",
    "copy-webpack-plugin": "^6.0.3",
